 <Form.Group controlId=\"\">\n            <Form.Label>description</Form.Label>\n            <Form.Control\n              type=\"description\"\n              placeholder=\"discription\"\n              onChange={(e) => {\n                setData({ ...data, description: e.target.value })\n              }}\n            />\n          </Form.Group>\n          <Form.Group controlId=\"\">\n            <Form.Label>Image Path</Form.Label>\n            <Form.Control\n              type=\"path\"\n              placeholder=\"imagePath\"\n              onChange={(e) => {\n                setData({ ...data, Poster: e.target.value })\n              }}\n            />\n          </Form.Group>\n          <Form.Group controlId=\"\">\n            <Form.Label>Rate</Form.Label>\n            <Form.Control\n              type=\"rate\"\n              placeholder=\"rate\"\n              onChange={(e) => {\n                setData({ ...data, rate: e.target.value })\n              }}\n            />\n          </Form.Group>\n          <Button variant=\"primary\" type=\"submit\">\n            Submit\n          </Button>\n        </Form>\n      </Row>\n    </Container>\n  )\n}\nexport default App\n","/home/azmi/gomycode/react-project/react-hooks/react-hooks/react-hooks/src/Components/MovieCard.js",[],"/home/azmi/gomycode/react-project/react-hooks/react-hooks/react-hooks/src/Components/MovieList.js",["34"],"/home/azmi/gomycode/react-project/react-hooks/react-hooks/react-hooks/src/Components/Filter.js",["35"],"import React from 'react'\nimport { useState, useEffect } from 'react'\n\nconst Search = (props) => {\n  const [searchValue, setSearchValue] = useState('')\n\n  const handleSearchInputChanges = (e) => {\n    setSearchValue(e.target.value)\n  }\n\n  const resetInputField = () => {\n    setSearchValue('')\n  }\n\n  const callSearchFunction = (e) => {\n    e.preventDefault()\n    props.search(searchValue)\n    resetInputField()\n  }\n\n  return (\n    <form className=\"search\">\n      <input\n